{
    "name": "esp-angular",
    "description": "Embedthis ESP for Angular 1.x",
    "version": "6.0.0",
    "warn": "This pak requires Angular 1 which is no longer supported.",
    "keywords": [
        "esp"
    ],
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/esp-angular.git"
    },
    "dependencies": {
        "angular-route": "~1.4",
        "angular-animate": "~1.4"
    },
    "devDependencies": {
        "pak": ">=1.0.0"
    },
    "ignore": [
        "doc**"
    ],
    "export": {
        "from": "*.js",
        "to": "esp-angular/"
    },
    "manage": "esp.json"
}
