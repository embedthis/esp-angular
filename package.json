{
    "name": "esp-angular",
    "description": "Embedthis ESP for Angular",
    "version": "5.0.0-rc0",
    "keywords": [
        "esp",
    ],
    "dependencies": {
        "esp-mvc": "~5.0",
        "angular": "~1.2"
    },
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "http://embedthis.com/licensing/"
        }
    ],
    "export": [
        {
            from: [
                "*.js",
            ],
            to: '${JSLIB}/esp-angular',
        }
    ],
    "client-scripts": [
        "${JSLIB}/esp-angular/*.js"
    ]
}
